yer1, setPlayer1] = useState(() => {\r\n    return pokemons.map((item) => ({\r\n      ...item,\r\n      possession: 'blue',\r\n    }));\r\n  });\r\n  const [player2, setPlayer2] = useState([]);\r\n  const [choiceCard, setChoiceCard] = useState(null);\r\n  const [steps, setSteps] = useState(0);\r\n  const [side, setSide] = useState(0);\r\n  const [stop, setStop] = useState(false);\r\n\r\n  const [resultType, setResultType] = useState(null);\r\n  const history = useHistory();\r\n\r\n  if (Object.keys(pokemons).length === 0) {\r\n    history.replace('/game');\r\n  }\r\n  useEffect(async () => {\r\n    const boardResponse = await fetch(\r\n      'https://reactmarathon-api.netlify.app/api/board'\r\n    );\r\n    const boardRequest = await boardResponse.json();\r\n    setBoard(boardRequest.data);\r\n\r\n    const player2Response = await fetch(\r\n      'https://reactmarathon-api.netlify.app/api/create-player'\r\n    );\r\n    const player2Request = await player2Response.json();\r\n    setPlayer2(() => {\r\n      return player2Request.data.map((item) => ({\r\n        ...item,\r\n        possession: 'red',\r\n      }));\r\n    });\r\n    setTimeout(() => {\r\n      setSide(randomSide);\r\n    }, 3000);\r\n\r\n  }, []);\r\n\r\n  const handleClickBoardPlate = async (position) => {\r\n    if (choiceCard) {\r\n      setStop(true);\r\n      const params = {\r\n        position,\r\n        card: choiceCard,\r\n        board,\r\n      };\r\n      const response = await fetch(\r\n        'https://reactmarathon-api.netlify.app/api/players-turn',\r\n        {\r\n          method: 'POST',\r\n          headers: {\r\n            'Content-Type': 'application/json',\r\n          },\r\n          body: JSON.stringify(params),\r\n        }\r\n      );\r\n      const request = await response.json();\r\n\r\n      if (choiceCard.player === 1) {\r\n        setPlayer1((prevState) =>\r\n          prevState.filter((item) => item.id !== choiceCard.id)\r\n        );\r\n      }\r\n      if (choiceCard.player === 2) {\r\n        setPlayer2((prevState) =>\r\n          prevState.filter((item) => item.id !== choiceCard.id)\r\n        );\r\n      }\r\n\r\n      setBoard(request.data);\r\n      setSteps((prevState) => {\r\n        const count = prevState + 1;\r\n        return count;\r\n      });\r\n      setSide(prevState => {\r\n        if (prevState === 1) {\r\n          return 2;\r\n        }\r\n        if (prevState === 2) {\r\n          return 1;\r\n        }\r\n      });\r\n    }\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (steps === 9) {\r\n      const [count1, count2] = counterWin(board, player1, player2);\r\n      if (count1 > count2) {\r\n        setResultType('win');\r\n      } else if (count1 < count2) {\r\n        setResultType('lose');\r\n      } else {\r\n        setResultType('draw');\r\n      }\r\n      // не знала как лучше придумать, чтобы результат увидеть\r\n      setTimeout(() => {\r\n        dispatch(setFinishBoard(board));\r\n        history.replace('/game/finish');\r\n      }, 5000);\r\n\r\n    }\r\n  }, [steps]);\r\n\r\n  return (\r\n    <div className={s.root}>\r\n      <Result type={resultType} />\r\n      <ArrowChoice side={side} stop={stop} />\r\n      <div className={s.playerOne}>\r\n        <PlayerBoard\r\n          player={1}\r\n          cards={player1}\r\n          onClickCard={(card) => {\r\n            if (side === 1) {\r\n              setChoiceCard(card)\r\n            }\r\n          }\r\n          }\r\n          disabled={side !== 1}\r\n        />\r\n      </div>\r\n      <div className={s.board}>\r\n        {board.map((item) => (\r\n          <div\r\n            key={item.position}\r\n            className={s.boardPlate}\r\n            onClick={() => !item.card && handleClickBoardPlate(item.position)}\r\n          >\r\n            {item.card && <PokemonCard {...item.card} isActive minimize />}\r\n          </div>\r\n        ))}\r\n      </div>\r\n      <div className={s.playerTwo}>\r\n        <PlayerBoard\r\n          player=